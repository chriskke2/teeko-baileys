{
  "name": "baileys-backend",
  "version": "1.0.0",
  "description": "WhatsApp chatbot using Baileys",
  "main": "lib/index.js",
  "scripts": {
    "start:local": "dotenv -e .env.local ts-node src/index.ts",
    "start:dev": "dotenv -e .env.development ts-node src/index.ts",
    "start": "dotenv -e .env.production ts-node src/index.ts",
    "build": "tsc",
    "messages": "node scripts/manage-predefined-messages.js",
    "migrate:segmentation": "node scripts/migrate-to-segmentation.js",
    "migrate:active-status": "node scripts/update-status-to-active.js",
    "check:segmentation": "node scripts/check-segmentation-data.js",
    "fix:country": "node scripts/fix-country-data.js",
    "test:trip-type": "node scripts/test-trip-type.js",
    "fix:onboarding": "node scripts/fix-onboarding-steps.js",
    "contexts": "node scripts/import-contexts.js",
    "context:import": "node scripts/import-contexts.js",
    "context:update-users": "node scripts/update-user-contexts.js",
    "context:test": "node scripts/test-context-generation.js",
    "context:check": "node scripts/check-context-collection.js"
  },
  "keywords": [
    "whatsapp",
    "chatbot",
    "baileys"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@cacheable/node-cache": "^1.4.0",
    "@hapi/boom": "^10.0.1",
    "@whiskeysockets/baileys": "^6.7.18",
    "axios": "^1.6.7",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.3",
    "multer": "^2.0.2",
    "open": "^8.4.2",
    "pino": "^9.2.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.32.6",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.14.2",
    "@types/open": "^6.0.0",
    "@types/qrcode": "^1.5.5",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/sharp": "^0.31.1",
    "@types/socket.io-client": "^1.4.36",
    "@types/uuid": "^10.0.0",
    "dotenv-cli": "^8.0.0",
    "pino-pretty": "^11.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
